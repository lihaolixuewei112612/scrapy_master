        window.history.forward(1);
        function confirmAtion() {
            var form = document.forms[0];
            if (form.saveCookies.checked) {
                var answer = confirm('安全提示：\n请勿在公用电脑上记录您的查询密码! \n如需清除用户名和密码，请点击“以其他用户登录”按钮。\n\n确认要在本机上记住您的用户名和密码吗？');
                if (answer) {
                    form.saveCookies.checked = true;
                }
                else {
                    form.saveCookies.checked = false;
                    return false;
                }
            }
            return true;
        }

        function login(form) {
            if (validateForm(form)) {
                document.getElementById('s').innerHTML = '正在登录系统，请稍等...';
                var enUS = document.createElement("div");
                enUS.innerHTML = 'Logging In...';
                document.getElementById('s').appendChild(enUS);
                return true
            }
            else
                return false;
        }
        function refreshImage() {
            var imgVeriCode = document.getElementById('imgVeriCode');
            imgVeriCode.src = '/veriCode.do?t=' + (new Date()).getTime();
            return false;
        }
        function my_reset(form) {
            form.reset();
            return false;
        }
        $(function () {
            var loginForm = $('form[name=loginForm]');
            var fieldUserID = loginForm.find('input[name=userID]');
            fieldUserID.get(0).focus();
            if (location.search.length > 0) {
                var elems = loginForm.find('input:not([type=hidden])');
                elems.val('');
            }

            var errorMsgContainer = $('.error-msg');
            var errorMsg = errorMsgContainer.text();
            errorMsgContainer.html((errorMsg || '').replace('|', '<br/>'));

            var btnRfreshCaptcha = $('.login-form-refresh-captcha-btn');
            btnRfreshCaptcha.on('click', refreshImage);

            window.loginForm_required = function() {
                var localeLabel = {
                    'zh_CN': {
                        'userID': '用户名',
                        'password': '密　码',
                        'vericode': '验证码'
                    },
                    'en_US': {
                        'userID': 'Username',
                        'password': 'Password',
                        'vericode': 'Verification Code'
                    }
                };
                var createMsg = function(field) {
                    return [
                        localeLabel.zh_CN[field] + ' 是必填字段',
                        'Error! ' + localeLabel.en_US[field] + ' is required field'
                    ].join('\n');
                }
                this.a0 = ["userID", createMsg('userID'), new Function ("varName", " return this[varName];")];
                this.a1 = ["password", createMsg('password'), new Function ("varName", " return this[varName];")];
                this.a2 = ["vericode", createMsg('vericode'), new Function ("varName", " return this[varName];")];
            }
        });
